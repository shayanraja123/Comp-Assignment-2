 Dim Word1 As String
        Dim Word2 As String
        Dim X As String
        Dim X2 As String
        Dim Y As String
        Dim Y2 As String

        Console.Write("Enter First Word:")
        Word1 = Console.ReadLine()
        Console.Write("Enter Second Word:")
        Word2 = Console.ReadLine()

        X = Left(Word1, 2)
        Y = Left(Word2, 2)
        X2 = Mid(Word1, 3)
        Y2 = Mid(Word2, 3)

        Console.WriteLine(Y & X2 & " " & X & Y2)

        Console.ReadLine()
